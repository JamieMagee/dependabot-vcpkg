      "version": "3.1.0"
    },
    {
      "name": "jsoncpp",
      "version": "1.9.5"
    },
    {
      "name": "cli11",
      "version": "2.4.2"
    },
    {
      "name": "cmark",
      "version": "0.30.3"
    }
  ],
  "builtin-baseline": "dd3097e305afa53f7b4312371f62058d2e665320",
  "vcpkg-configuration": {
    "overlay-ports": [
      "./dep/vcpkg-overlay-ports"
    ]
